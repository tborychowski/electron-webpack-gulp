@import "lib"

#app
	position absolute
	top 0
	left 0
	right 0
	bottom 0
	background color-bg-dark0
	color color-text-light2
	overflow hidden
	padding 0


h1, h2, h3, h4, h5, h6
	margin 0
	b 0

.scroller
	flex 1
	overflow hidden
	overflow-y auto
	-webkit-overflow-scrolling touch

.flex-filler
	flex 1
